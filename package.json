{
  "name": "restapi",
  "version": "1.0.0",
  "main": "server/server.js",
  "license": "MIT",
  "scripts": {
    "build-server": "babel server -d dist-server --copy-files",
    "start-server": "node dist-server/server.js",
    "clean-server-dist": "rimraf dist",
    "server": "npm-run-all clean-server-dist build-server start-server && npm run build-client",
    "dev-server": "nodemon server/server.js --exec @babel-node",
    "test": "mocha --require  @babel/polyfill --require @babel/register",
    "dev-client": "cross-env NODE_ENV=development env-cmd -f ./.env.development webpack-dev-server --config webpack.development.config.js --open --mode development",
    "build-client": "cross-env NODE_ENV=production env-cmd -f ./.env.production webpack -p --mode production --config ./webpack.config.js",
    "prod": "node client/server.js"
  },
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.4",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.4.0",
    "eslint-plugin-react": "^7.20.3",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "history": "^5.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "img-loader": "^3.0.1",
    "immutable": "^4.0.0-rc.12",
    "mime": "^2.4.6",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loading-skeleton": "^2.1.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "rimraff": "^0.0.1-security",
    "sequelize": "^6.3.0",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.3",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.0.9"
  }
}
